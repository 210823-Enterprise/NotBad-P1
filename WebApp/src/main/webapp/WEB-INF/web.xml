<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://JAVA.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>NotBadServlets</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<context-param>
		<param-name>contextExample</param-name>
		<param-value>Global Pizza Chain</param-value>
	</context-param>

	<servlet>
		<display-name>PlayServlet</display-name>
		<servlet-name>PlayServlet</servlet-name>
		<servlet-class>com.revature.sessionservlet.PlayServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PlayServlet</servlet-name>
		<url-pattern>/playserv</url-pattern>
	</servlet-mapping>


	<servlet>
		<display-name>EditServlet</display-name>
		<servlet-name>EditServlet</servlet-name>
		<servlet-class>com.revature.sessionservlet.EditServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditServlet</servlet-name>
		<url-pattern>/editserv</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>DeleteServlet</display-name>
		<servlet-name>DeleteServlet</servlet-name>
		<servlet-class>com.revature.sessionservlet.DeleteServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteServlet</servlet-name>
		<url-pattern>/deleteserv</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>ConfirmDeleteServlet</display-name>
		<servlet-name>ConfirmDeleteServlet</servlet-name>
		<servlet-class>com.revature.sessionservlet.ConfirmDeleteServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ConfirmDeleteServlet</servlet-name>
		<url-pattern>/confirmdeleteserv</url-pattern>
	</servlet-mapping>
	


	<servlet>
		<display-name>HomeServlet</display-name>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>com.revature.sessionservlet.HomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/homeserv</url-pattern>
	</servlet-mapping>


	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.revature.sessionservlet.LoginServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/loginserv</url-pattern>
	</servlet-mapping>

	<listener>
		<listener-class>
			com.revature.util.ServletInitilizer
		</listener-class>
	</listener>
	
</web-app>